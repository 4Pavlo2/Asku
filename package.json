{
  "name": "app",
  "dependencies": {
    "@popperjs/core": "^2.11.2",
    "@rails/actioncable": "^7.0.2-2",
    "@rails/activestorage": "^7.0.2-2",
    "@rails/ujs": "^7.0.2-2",
    "autoprefixer": "^10.4.2",
    "bootstrap": "^5.1.3",
    "esbuild": "^0.14.23",
    "sass": "^1.49.8",
    "turbolinks": "^5.2.0"
    "@rails/webpacker": "5.4.0",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12"
  },
  "version": "0.1.0",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "build:css:dev": "sass ./app/assets/stylesheets/application.bootstrap.scss ./app/assets/builds/application.css --no-source-map --load-path=node_modules",
    "build:css": "build:css:dev && postcss ./app/assets/builds/application.css --replace --use autoprefixer --no-map",
    "build": "esbuild app/javascript/*.* --bundle --minify --outdir=app/assets/builds"
  },
  "packageManager": "yarn@3.1.1"
}
